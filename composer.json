{
  "name": "supermundano/sage-the-events-calendar",
  "type": "library",
  "license": "MIT",
  "description": "",
  "homepage": "https://github.com/supermundano/sage-the-events-calendar",
  "authors": [
    {
      "name": "Carlos Faria",
      "email": "carlos@supermundano.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Supermundano\\Sage\\TheEventsCalendar\\": "src/"
    }
  },
  "require": {
    "roots/acorn": "*"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "~3.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": [
      "phpcs --ignore=vendor --extensions=php --standard=PSR2 ."
    ]
  },
  "archive" : {
    "exclude": [
      ".gitignore"
    ]
  },
  "extra": {
    "acorn": {
      "providers": [
        "Supermundano\\Sage\\TheEventsCalendar\\TheEventsCalendarServiceProvider"
      ]
    }
  }
}
